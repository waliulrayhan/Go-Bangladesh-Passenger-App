{
  "expo": {
    "name": "Go Bangladesh",
    "slug": "go-bangladesh",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appIconPng1024_lg.png",
    "userInterfaceStyle": "light",
    "primaryColor": "#4A90E2",
    "splash": {
      "image": "./assets/images/splashScreenTransparent1024_updated.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gobangladesh.passenger",
      "icon": "./assets/images/iOS.png",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIStatusBarStyle": "UIStatusBarStyleLightContent",
        "UIViewControllerBasedStatusBarAppearance": false,
        "UIStatusBarHidden": false,
        "NSLocationWhenInUseUsageDescription": "This app needs access to location to show your current position on the map and help you find nearby buses.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to location to show your current position on the map and help you find nearby buses.",
        "NSLocationTemporaryUsageDescriptionDictionary": {
          "MapLocation": "This app needs location access to show your position on the map."
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appIconPng1024_lg.png",
        "backgroundColor": "#FFFFFF"
      },
      "splash": {
        "image": "./assets/images/splashScreenTransparent1024_updated.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.gobangladesh.passenger"
    },
    "web": {
      "favicon": "./assets/images/appIconPng1024_lg.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "This app needs access to location to show your current position on the map and help you find nearby buses.",
          "locationWhenInUsePermission": "This app needs access to location to show your current position on the map and help you find nearby buses.",
          "isIosBackgroundLocationEnabled": false,
          "isAndroidBackgroundLocationEnabled": false
        }
      ],
      "expo-font",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splashScreenTransparent1024_updated.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFFFFF"
        }
      ],
      "expo-secure-store"
    ],
    "scheme": "go-bangladesh",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "51d8d954-675f-4cf9-a013-3b6e6b38620a"
      }
    }
  }
}
