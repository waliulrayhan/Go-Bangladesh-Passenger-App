{
  "expo": {
    "name": "Go Bangladesh",
    "slug": "go-bangladesh",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appIconPng1024_lg.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splashScreenTransparent1024_updated.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.solutionspin.gobangladesh",
      "bundleName": "Go BD",
      "displayName": "Go BD",
      "icon": "./assets/images/iOS.png",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appIconPng1024_lg.png",
        "backgroundColor": "#FFFFFF"
      },
      "splash": {
        "image": "./assets/images/splashScreenTransparent1024_updated.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.solutionspin.gobangladesh"
    },
    "web": {
      "favicon": "./assets/images/appIconPng1024_lg.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "This app needs access to location for trip tracking."
        }
      ],
      "expo-font",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splashScreenTransparent1024_updated.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFFFFF"
        }
      ]
    ],
    "scheme": "go-bangladesh",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "51d8d954-675f-4cf9-a013-3b6e6b38620a"
      }
    }
  }
}
