    "version:patch": "npm version patch && node -e \"const fs=require('fs'); const app=JSON.parse(fs.readFileSync('app.json')); app.expo.version=require('./package.json').version; fs.writeFileSync('app.json', JSON.stringify(app,null,2))\"",
